# Environment variables for the dashboard
# Copy this file to .env.local and replace with your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
DATABASE_URL=postgresql://postgres:password@your-host:5432/postgres

# Dashboard Authentication
NEXT_PUBLIC_DASHBOARD_PASSWORD=your_secure_password_here